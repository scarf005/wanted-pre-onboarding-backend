{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["ESNext", "dom", "esnext.disposable"],
		"moduleResolution": "Bundler",
		"noEmit": true,
		"esModuleInterop": true,
		"allowImportingTsExtensions": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
